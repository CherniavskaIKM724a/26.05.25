#include <iostream>
#include <windows.h>
#include <vector>

using namespace std;

int main()
{
    SetConsoleCP(1251);            // +кириллица
    SetConsoleOutputCP(1251);

    vector<int> numbers;

    for (int i = 1; i <= 5; ++i) {
        numbers.push_back(i); // Добавляем элемент в конец
    }

    for (int i = 0; i < numbers.size(); i++) {
        std::cout << numbers[i] << " ";
    }
    return 0;
}
